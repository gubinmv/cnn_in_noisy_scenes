#!/bin/bash

echo -e "\033[32;1;40m ====== START TRAIN =======================\033[0m"

for ((i=0; i<=40; i++))
	do
	echo -e "\033[32;1;40m ==========================================\033[0m"
	echo -e "\033[32;1;40m       start python with index = " $i "\033[0m"
	python3.6 new4_train.py $i
	done

echo -e "\033[32;1;40m ==========================================\033[0m"
echo -e "\033[32;1;40m ====== END TRAIN =========================\033[0m"























